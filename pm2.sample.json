{
  "name": "shard",
  "script": "server/app.js",
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "exec_mode": "cluster",
  "node_args": "--harmony",
  "instances": 0,
  "min_uptime": "200s",
  "max_memory_restart": "500M",
  "watch": false,
  "merge_logs": true,
  "autorestart": true,
  "_": "_",
  "error_file": "/tmp/shard.stderr.log",
  "out_file": "/tmp/shard.stdout.log"
}
